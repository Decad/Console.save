{
    "manifest_version": 2,
    "name": "console.save",
    "description": "Adds console.save to every page, allows saving of json from the console.",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "run_at": "document_start",
          "js": ["bootstrap.js"]
        }
    ],
    "web_accessible_resources": [
        "console-save.js"
    ]
}